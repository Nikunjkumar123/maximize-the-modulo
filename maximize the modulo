#include<bits/stdc++.h>
#include<iostream>
#include<sstream>
#include<cmath>
#include<string>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        int m,k;
        cin>>m>>k;
        char str[m] = {};
        for(int i=0;i<m;i++){
            cin>>str[i];
        }
        std::string str1 = std::to_string(str[m]);
        str1.pop_back();
        int ans;
        ans = str[m]%k;
        cout<<ans+4<<endl;
        return 0;
    }
}
